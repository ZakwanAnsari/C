#include <stdio.h>
 
void main() {
    int a,b,c;
    printf("Enter value a, b, c \n");
    scanf("%d %d %d",&a, &b ,&c);
    if(a>b && a>c)
        printf("Value 'a' is the greater number");
    else
    {
        if(b>c)
            printf("Value 'b' is the greater number");
        else
            printf("Value 'c' is the greater number");
    }

}
