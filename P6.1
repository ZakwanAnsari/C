Practical No. 6.1: To check whether the number is greater than, less
than or equal to 5
*/
#include <stdio.h>
 
void main() {
    int a;
    printf("Enter variable a: ");
    scanf("%d",&a);
    if(a==5)
        printf("Your variable is equal to 5");
    else
    {
        if(a>5)
            printf("Your variable is greater than 5");
        else
            printf("Your variable is less than 5");
    }

}
