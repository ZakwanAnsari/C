// sorting of array in ascending 
#include <stdio.h>

void main() 
{
    int a[5];
    int i=0;
    int j=0;
    int temp=0;
    
        //Initization
        for(i=0;i<5;i++)
        {
             printf("Enter %d array elemtents: ",i+1);
             scanf("%d",&a[i]);
        }   
    
        //Logic of ascending
        for(i=0;i<5;i++)
        {
             for(j=i+1;j<5;j++)
             {
                 if(a[i] > a[j])
                 {
                     
                     temp=a[i];
                     a[i]=a[j];
                     a[j]=temp;
                 }
                
             }
        }   
         //OUTPUT
        for(i=0;i<5;i++)
        {
               printf("%d\t",a[i]);
        }  
    
    }   
   
   
    
   
