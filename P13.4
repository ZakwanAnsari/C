//P13.4:String functions(strcat)
//Append last string to first
#include <stdio.h>
#include <string.h>

void main() 
{
    char a[20],b[20];
    printf("Enter variable a:");
    scanf("%s",&a);
    printf("Enter variable b:");
    scanf("%s",&b);
    strcat(a,b);    //concatenates
    printf("a=%s",a);
    
}
